   button {
  /* Generic styles for a button */
    cursor: pointer;
    width: 200px;
    margin: 10px auto;
    font-size: 14px;
    color: #444;
    background-color: transparent;
    border: 2px solid #0d203a;
    border-radius: 0.1em;
    padding: 1.3em 2em;
    position: relative;
    z-index: 1;
    overflow: hidden;
    font-weight: 700;
    text-transform: uppercase;
    /*letter-spacing: 0.15em;*/
    font-family: "Poppins", sans-serif !important;
}

button:hover {
  /* Generic Hover style for a button */
  color: #fff;
}

button::after {
  /* Adds the overlay for creating slide effect */
  content: '';
  position: absolute;
  background-color: #0d203a;
  z-index: -1;
  padding: 0.25em 0.5em;
  display: block;
  transition: all 0.35s;
}

button:hover::after {
  /* Hover effect for the overlay slide effect */
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  transition: all 0.7s;
}

.left-slide::after {
  top: 0;
  bottom: 0;
  left: -100%;
  right: 100%;
}

